{
    
        "version": 2,
        "builds": [
          {
            "src": "index.js",
            "use": "@now/node-server"
          }],
          "routes": [
            {
              "src": ["/menuDinner","menuBreakfast"],
              "dest": "index.js",
              "methods": ["GET"]
            },
            {
              "src": "/menuBreakfast",
              "dest": "index.js",
              "methods": ["GET"]
            }
          ]
        ,
        "build": {
          "env": {
            "token": "@token",
            "mongourl": "@mongourl"
        }
      }      
  
}